{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "require-await": "off",
        "semi": "error",
        "no-console":"off",
        "no-unused-vars": "error"
    }
}